{
    "name": "Discord cookies and localStorage extractor",
    "description": "Extracts cookies and localStorage from Discord",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["activeTab", "scripting", "tabs", "cookies"],
    "action": {
        "default_title": "Discord cookies and localStorage extractor"
    },
    "content_scripts": [
        {
            "matches": ["*://discord.com/*"],
            "js": ["js/content_script.js"]
        }
    ],
    "host_permissions": ["*://discord.com/*"],
    "background": {
        "service_worker": "js/background.js"
    }
}
